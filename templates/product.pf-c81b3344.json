        "1b2deae3-1f76-4ad2-a78a-11f88fe8b9d1": {
          "type": "text",
          "disabled": true,
          "settings": {
            "hide_mobile": false,
            "text": "<p>Be Part of the Change</p>",
            "padding_top": 0,
            "padding_bottom": 2.7,
            "text_style": "subtitle",
            "text_weight": "bold",
